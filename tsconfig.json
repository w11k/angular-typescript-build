{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "rootDir": "src",
    "target": "es5"
    //,
//    "mapRoot": "/"
    //    "sourceMap": true,
    //    "inlineSources": true
  }
}
